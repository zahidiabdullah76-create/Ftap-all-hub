local Button = Tab:CreateButton({
   Name = "Load Noxi Hub A.k.a My hub Key:Free_239832347xkxn3289",
   Callback = function()
      -- ==============================
-- SERVICES / BASE VARS
-- ==============================
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local Workspace = game:GetService("Workspace")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local RunService = game:GetService("RunService")

local plr = Players.LocalPlayer
local cam = Workspace.CurrentCamera

-- ==============================
-- MAIN TOGGLES / VALUES
-- ==============================
local infLineExtendT = false
local antiGrabT = false
local antiGrabAnchorT = true
local increaseLineExtendV = 1
local lineDistanceV = 11

local aimLock = false
local currentTarget = nil
local MAX_DISTANCE = 1500
local SMOOTHNESS = 0.2
local TOGGLE_KEY = Enum.KeyCode.F

-- ==============================
-- GUI SETUP
-- ==============================
local MainWindow = Rayfield:CreateWindow({
    Name = "MainWindow",
    LoadingTitle = "Rayfield Interface Suite",
    LoadingSubtitle = "by Noxi",
    ShowText = "Rayfield",
    Theme = "Default",
    ToggleUIKeybind = "K",
    ConfigurationSaving = {Enabled = true, FileName = "Noxi_Ftap"},
    KeySystem = true,
    KeySettings = {
        Title = "Noxi's Keysystem",
        FileName = "Key",
        SaveKey = true,
        GrabKeyFromSite = true,
        Key = {"Free_239832347xkxn3289"}
    }
})

-- ==============================
-- MAIN TAB
-- ==============================
local MainTab = MainWindow:CreateTab("Main", 4483362458)

-- Infinite Line Extend Toggle
local ExtendLineToggle = MainTab:CreateToggle({
    Name = "Infinite Line Extend",
    CurrentValue = infLineExtendT,
    Flag = "LineExtendToggle",
    Callback = function(value)
        infLineExtendT = value
    end
})

-- Line Distance Input (no limit)
local LineDistanceInput = MainTab:CreateInput({
    Name = "Line Distance Increment",
    CurrentValue = tostring(increaseLineExtendV),
    PlaceholderText = "Enter any number",
    RemoveTextAfterFocusLost = false,
    Flag = "LineDistanceInput",
    Callback = function(text)
        local val = tonumber(text)
        if val then
            increaseLineExtendV = val
        else
            LineDistanceInput:SetText(tostring(increaseLineExtendV))
        end
    end
})

-- Anti Grab Toggle
local AntiGrabToggle = MainTab:CreateToggle({
    Name = "Anti Grab",
    CurrentValue = antiGrabT,
    Flag = "AntiGrabToggle",
    Callback = function(value)
        antiGrabT = value
        if value then
            task.spawn(function()
                local char = plr.Character or plr.CharacterAdded:Wait()
                local hrp = char:WaitForChild("HumanoidRootPart")
                while antiGrabT do
                    if plr:FindFirstChild("IsHeld") and plr.IsHeld.Value then
                        if antiGrabAnchorT then
                            hrp.Anchored = true
                            while plr.IsHeld.Value do
                                ReplicatedStorage.CharacterEvents.Struggle:FireServer(plr)
                                task.wait(0.01)
                            end
                            hrp.Anchored = false
                        else
                            while plr.IsHeld.Value do
                                ReplicatedStorage.CharacterEvents.Struggle:FireServer(plr)
                                task.wait(0.01)
                            end
                        end
                    end
                    task.wait(0.1)
                end
            end)
        end
    end
})

-- Mouse Wheel Line Extend
UserInputService.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseWheel and infLineExtendT then
        if lineDistanceV < 11 then lineDistanceV = 11 end
        if input.Position.Z > 0 then
            lineDistanceV = lineDistanceV + increaseLineExtendV
        elseif input.Position.Z < 0 then
            lineDistanceV = lineDistanceV - increaseLineExtendV
        end
    end
end)

-- Grab Parts Extender
Workspace.ChildAdded:Connect(function(child)
    if child.Name == "GrabParts" and child:IsA("Model") then
        if infLineExtendT and UserInputService.MouseEnabled then
            child:WaitForChild("GrabPart")
            child:WaitForChild("DragPart")
            local clonedDragPart = child.DragPart:Clone()
            clonedDragPart.Name = "DragPart1"
            clonedDragPart.AlignPosition.Attachment1 = clonedDragPart.DragAttach
            clonedDragPart.Parent = child
            lineDistanceV = (clonedDragPart.Position - cam.CFrame.Position).Magnitude
            clonedDragPart.AlignOrientation.Enabled = false
            child.DragPart.AlignPosition.Enabled = false
            task.spawn(function()
                while child.Parent do
                    clonedDragPart.Position = cam.CFrame.Position + cam.CFrame.LookVector * lineDistanceV
                    task.wait()
                end
                lineDistanceV = 0
            end)
        end
    end
end)

-- ==============================
-- AIM LOCK
-- ==============================
local Mouse = plr:GetMouse()
local function getClosestPlayerToMouse()
    local closestPlayer = nil
    local shortestDistance = MAX_DISTANCE
    for _, player in ipairs(Players:GetPlayers()) do
        if player ~= plr and player.Character then
            local head = player.Character:FindFirstChild("Head")
            local hum = player.Character:FindFirstChild("Humanoid")
            if head and hum and hum.Health > 0 then
                local screenPos, onScreen = cam:WorldToViewportPoint(head.Position)
                if onScreen then
                    local distance = (Vector2.new(Mouse.X, Mouse.Y) - Vector2.new(screenPos.X, screenPos.Y)).Magnitude
                    if distance < shortestDistance then
                        shortestDistance = distance
                        closestPlayer = player
                    end
                end
            end
        end
    end
    return closestPlayer
end

UserInputService.InputBegan:Connect(function(input, gpe)
    if gpe then return end
    if input.KeyCode == TOGGLE_KEY then
        aimLock = not aimLock
        if aimLock then
            currentTarget = getClosestPlayerToMouse()
        else
            currentTarget = nil
        end
    end
end)

RunService.RenderStepped:Connect(function()
    if aimLock and currentTarget and currentTarget.Character then
        local head = currentTarget.Character:FindFirstChild("Head")
        local hum = currentTarget.Character:FindFirstChild("Humanoid")
        if head and hum and hum.Health > 0 then
            cam.CFrame = cam.CFrame:Lerp(CFrame.new(cam.CFrame.Position, head.Position), SMOOTHNESS)
        else
            currentTarget = nil
        end
    end
end)

